{
  "name": "@tago-io/snippets",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "tsx scripts/prepare-data.ts && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "fmt": "biome format --write . && ruff format snippets/analysis/",
    "fmt:js": "biome format --write .",
    "fmt:py": "ruff format snippets/analysis/",
    "lint": "biome lint . && ruff check snippets/analysis/",
    "lint:js": "biome lint .",
    "lint:py": "ruff check snippets/analysis/",
    "lint:fix": "biome lint --write . && ruff check --fix snippets/analysis/"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.13.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.8",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
